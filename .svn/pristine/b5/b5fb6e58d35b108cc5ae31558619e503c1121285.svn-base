*{padding:0px; margin:0px; list-style:none;}
body {font:normal 14px/1.231 "微软雅黑", Arial, SimSun, sans-serif; color:#555; background:url("/front/images/manager/tts/bg.jpg") repeat scroll 0 0 transparent;}
fieldset, img ,abbr, acronym{ border:0; }
table { border-spacing:0; }
ol, ul { list-style:none;}
address, caption, cite, code, dfn, em, th, var { font-style:normal; font-weight:400; }
strong { font-style:normal; font-weight:700; }
caption, th { text-align:left; }
h1, h2, h3, h4, h5, h6 { font-size:16px; font-weight:400; }
q:before, q:after { content:""; }
table{ border-collapse:collapse; font-size:12px;}
a { color:#0069ca; text-decoration:none;}
.fL { float:left; }
.fR { float:right; }
/*yiliu=start*/
.tB { font-weight:700; }
.tN { font-weight:400; }
.aL { text-align:left; }
.aC { text-align:center; }
.aR { text-align:right; }
.clr, .clear { clear:both; height:0; overflow:hidden; }

.clearfix{display:block; *display:inline-block;}
.clearfix:after{ clear:both; content:"."; display:block; height:0; overflow:hidden; visibility:hidden;}

.cPrange { color:#f60; }
.cBlue { color:#0069ca; }
.cGreen { color:#84be3c; }
.hide{ display:none;}
.pr{ position:relative;}
.ma{ margin:0 auto;}

