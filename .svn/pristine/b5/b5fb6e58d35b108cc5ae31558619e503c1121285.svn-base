/*浮层*/
.ffpos{position:fixed; _position:absolute; height:100px; padding:15px 0px 0px 0px; background:#000; filter:alpha(opacity=80); -moz-opacity:0.8; opacity:0.8; left:0px; bottom:0px; width:100%; z-index:888;}
.ffpos .midmpos{width:1000px; margin:0px auto;}
.midmpos .postxt{height:30px; line-height:30px; color:#ebebeb; font-size:16px; text-align:center;}
.midmpos .posbtn{height:50px; padding-left:200px; line-height:40px; padding-top:10px;}
.midmpos .posbtn a{margin-left:20px; font-size:16px;}
.abtn02{height:42px; line-height:42px; width:100px; text-align:center; background:#ff9501; border:1px #000 solid; border-radius:3px; font-size:16px; color:#000;}
.abtn02:hover{ color:#fff; background:#ff8401;}
.grft{color:#ebebeb;}

/*top*/
.wrap{ width:990px; margin:12px auto 0px auto;}
.top{ height:84px; background:url(/front/images/manager//tts/headbg.jpg) repeat-x;}

/*header*/
.header{ height:26px; border-bottom:1px solid #7eaff1; width:965px; margin:0 auto; padding-right:25px; text-align:right; line-height:26px;}
.header span,.header a{padding-right:10px;}
.header a.npadrt{padding-right:0px;}
.header span{ color:#446085;}
.header a{ color:#fff;}
.header a:hover{ color:#446085;}

/*nav*/
.nav{ height:56px; border-top:1px solid #b1d3ff; width:990px; margin:0 auto;}
.adminlogo{width:199px; height:25px; display:bloak; float:left; background:url(/front/images/manager/ttslogo.jpg) no-repeat; margin-top:15px; margin-left:10px; }
.nav div{float:right; width:750px; height:51px; padding-top:5px; text-align:right;}
.nav div span{ display:inline-block; height:51px; width:113px; padding-right:12px; background:url(/front/images/manager//tts/navbg.gif) right -56px no-repeat;}
.nav div span.nobg{background:none;}
.nav div span a{ color:#fff; display:block; height:51px; line-height:51px; text-align:center; font-family:"微软雅黑"; font-size:16px;}
.nav div span a.on{ color:#005bac; background:url(/front/images/manager//tts/navbg.gif) 0px 0px no-repeat;}
.nav div span a:hover{color:#005bac;}

/*left*/
.left{ width:160px; float:left;}
.left h3{ font:14px microsoft yahei,Arial; height:34px; margin-bottom:5px;}
.left h3 a{ display:block; height:34px; padding-left:30px; line-height:34px; background:url(/front/images/manager//tts/lfnav.jpg) no-repeat 0px -34px; color:#555;}
.left h3 a:hover{color:#005bac;}
.left h3 a.on{font-weight:bold; color:#FFF; background:url(/front/images/manager//tts/lfnav.jpg) no-repeat 0px 0px;}
.left h3 a.secmn{background:url(/front/images/manager//tts/lfnav.jpg) no-repeat 0px -124px;}
.left h3.tree-node,.left h3 a.secmn:hover{margin-bottom:0px; background:url(/front/images/manager//tts/lfnav.jpg) no-repeat 0px -158px;}
.left h3.tree-node{height:34px; padding-left:30px; line-height:34px;}
.left ul{background:#fefefe; border:1px #ddd solid; border-top:0px;}
.left ul li{ display:block; height:34px; font-size:14px; position:relative;}
.left ul li a{color:#555; display:block; height:33px; border-bottom:1px solid #EEE; line-height:33px; text-indent:33px; font-family:microsoft yahei,Arial;}
.left ul li a:hover{ background-color:#f2f5ff;}
.left ul li a.on{font-weight:bold; color:#FFF; height:34px; border-bottom:0px; background:url(/front/images/manager//tts/lfnav.jpg) no-repeat 0px 0px;}
.left ul li em.message_num{ display:block; height:19px; line-height:16px; width:38px; background:url(/front/images/manager/sprite.png) no-repeat -101px 0; position:absolute; left:135px; top:3px; font-size:12px; color:#fff;}
.left ul li a.nobd{border-bottom:0px; height:34px;}

/*right*/
.right{ width:822px; float:right;}

/*formhtml*/

.mandiv h2.title{ background:#e4ecf9; height:38px; line-height:38px; display:block; border-radius: 3px 3px 0 0; border:1px solid #cbdbf2; padding-left:20px; color:#005aac;}
.maninput{position:relative; background:#fff; border:1px #cbdbf2 solid; border-top:0px;}
.maninput table{ margin:22px 24px 17px; position:relative; *margin:8px 24px 17px; }
.maninput table th{ text-align:right; min-width:100px;}
.maninput table td{ height:30px; line-height:30px;}
.maninput table td img{ vertical-align:middle;}


