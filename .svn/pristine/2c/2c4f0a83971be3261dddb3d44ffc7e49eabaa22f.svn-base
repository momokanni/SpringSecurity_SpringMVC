package com.caishen91.jupiter.model;

import java.math.BigDecimal;

/**
 * 投资人账户信息
 */
public class FaeInvestorAccount {

    private int id;

    private int investorId;

    private BigDecimal balance;

    private BigDecimal toCollectPrincipal;

    private BigDecimal toCollectInterest;

    private BigDecimal frozenAmount;

    private BigDecimal totalInvest;

    private BigDecimal totalIncome;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getInvestorId() {
        return investorId;
    }

    public void setInvestorId(int investorId) {
        this.investorId = investorId;
    }

    public BigDecimal getBalance() {
        return balance;
    }

    public void setBalance(BigDecimal balance) {
        this.balance = balance;
    }

    public BigDecimal getToCollectPrincipal() {
        return toCollectPrincipal;
    }

    public void setToCollectPrincipal(BigDecimal toCollectPrincipal) {
        this.toCollectPrincipal = toCollectPrincipal;
    }

    public BigDecimal getToCollectInterest() {
        return toCollectInterest;
    }

    public void setToCollectInterest(BigDecimal toCollectInterest) {
        this.toCollectInterest = toCollectInterest;
    }

    public BigDecimal getFrozenAmount() {
        return frozenAmount;
    }

    public void setFrozenAmount(BigDecimal frozenAmount) {
        this.frozenAmount = frozenAmount;
    }

    public BigDecimal getTotalInvest() {
        return totalInvest;
    }

    public void setTotalInvest(BigDecimal totalInvest) {
        this.totalInvest = totalInvest;
    }

    public BigDecimal getTotalIncome() {
        return totalIncome;
    }

    public void setTotalIncome(BigDecimal totalIncome) {
        this.totalIncome = totalIncome;
    }
}
