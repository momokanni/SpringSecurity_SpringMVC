.valuecomb .decrease,.valuecomb .increase{background:url(/front/images/skin.png) 0 0 no-repeat;}
.valuecomb { position:relative;float:left; display:inline; margin-right:5px; padding:2px 0; }
.valuecomb .decrease { float:left; display:inline; margin-top:3px; width:15px; height:15px; background-position:-75px -43px; cursor:pointer; }
.valuecomb .increase { float:left; display:inline; margin-top:3px; width:15px; height:15px;background-position:-100px -43px; cursor:pointer; }
.valuecomb .ct { float:left; display:inline; margin:0 3px; }
.valuecomb .ct .textbox { float:left; display:inline; margin:0; padding:2px 0; border:1px solid #d5d5d5; width:30px; text-align:center; vertical-align:middle; background-color:#fff; outline:none; }
.valuecomb .ct .textbox:focus,
.valuecomb .ct .focus { border-color:#aeaeae; }
.valuecomb .decrease_hover { background-position:-25px -43px; }
.valuecomb .increase_hover { background-position:-50px -43px; }
.valuecomb .decrease_disable { background-position:-125px -43px; cursor:default; }
.valuecomb .increase_disable { background-position:-150px -43px; cursor:default; }
.valuecomb .ct .textbox_disable { cursor:default; }
.valuecomb .hint {
	background-color: #FFDAA1;
    left: -29px;
    position: absolute;
    top: 24px;
    white-space: normal;
}
.valuecomb .hint .arrow {
	background-position:-15px -28px;
	width:10px; 
	height:5px; 
	position:absolute; 
	top:0; 
	z-index:2
}
.valuecomb .sub .arrow{
	left: 32px
}
.valuecomb .add .arrow{
	left: 85px
}
.valuecomb .hint .hintContent {
	background-color: #FFFEDF;
    border: 1px solid #F8D3A7;
    color: #000000;
    left: 0;
    line-height: 22px;
    padding: 5px 5px 5px 9px;
    position: absolute;
    top: 4px;
    width: 110px;
    word-break: break-all;
    word-wrap: break-word;
    z-index: 1;
    text-align: center;
}

