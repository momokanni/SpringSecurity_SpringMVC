package com.caishen91.jupiter.enums;

public enum SysRole {
	admin(1, "平台管理员"),
	user(2, "运营")
	;
	
	private int role;
	
	private String name;
	
	private SysRole(int role, String name) {
		this.role = role;
		this.name = name;
	}

	public int getRole() {
		return role;
	}

	public void setRole(int role) {
		this.role = role;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
