package com.caishen91.jupiter.vo;

import java.util.Date;

/**
 * 	移动端文章列表VO
 * @author Administrator
 *
 */
public class MobileARListVO {

	// 文章ID
	private String arId;
	// 标题
	private String title;
	// 阅读量
	private String readVolume;
	// 点赞量
	private String laudVolume;
	// 转发量
	private String forwardVolume;
	// 公众号头像
	private String blogImg;
	// 首图
	private String headImg;
	// 公众号名称
	private String blogName;
	// 发布时间
	private Date time;
	// 
	private String timeDesc;
	
	public String getArId() {
		return arId;
	}
	public void setArId(String arId) {
		this.arId = arId;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getReadVolume() {
		return readVolume;
	}
	public void setReadVolume(String readVolume) {
		this.readVolume = readVolume;
	}
	public String getForwardVolume() {
		return forwardVolume;
	}
	public void setForwardVolume(String forwardVolume) {
		this.forwardVolume = forwardVolume;
	}
	public String getBlogImg() {
		return blogImg;
	}
	public void setBlogImg(String blogImg) {
		this.blogImg = blogImg;
	}
	public String getHeadImg() {
		return headImg;
	}
	public void setHeadImg(String headImg) {
		this.headImg = headImg;
	}
	public String getLaudVolume() {
		return laudVolume;
	}
	public void setLaudVolume(String laudVolume) {
		this.laudVolume = laudVolume;
	}
	public Date getTime() {
		return time;
	}
	public void setTime(Date time) {
		this.time = time;
	}
	public String getTimeDesc() {
		return timeDesc;
	}
	public void setTimeDesc(String timeDesc) {
		this.timeDesc = timeDesc;
	}
	public String getBlogName() {
		return blogName;
	}
	public void setBlogName(String blogName) {
		this.blogName = blogName;
	}
}
