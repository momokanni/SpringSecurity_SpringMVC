@charset "utf-8";
/* CSS Document */

body {
  color: #333333;
  font-family: "Open Sans", sans-serif;
  padding: 0px !important;
  margin: 0px !important;
  direction: "ltr";
  font-size: 14px; }
  
*{ padding:0; margin:0; list-style:none}

.portlet-body {padding: 30px; }
	
.clearfix {zoom: 1;}
.clearfix { display: block; *display:inline-block;}
.clearfix:after { clear: both;  content: ".";  display: block;  height: 0;  overflow: hidden;  visibility: hidden;}

.dn{ display:none}

.ovet{ height:auto; overflow:hidden}
.aut{ margin:0 auto}

.fl{ float:left}
.fr{ float:right}
.pr{ position:relative}

.imgaut{ display:block; margin:0 auto; width:100%}

/*width*/
.wt1{ width:100%}
.wt2{ width:50%}
.wt3{ width:33.3%}
.wt4{ width:25%}
.wt5{ width:20%}

/*color*/
.colf{ color:#fff}
.colc{ color:#ccc}
.colb{ color:#bbb}
.cola{ color:#aaa}
.col9{ color:#999}
.col8{ color:#888}
.col6{ color:#666}
.col4{ color:#444}
.col3{ color:#333}
.colq{ color:#8896a0}

.colred{ color:#f00}
.colblue{ color:#3598dc}
.colgreen{ color:#7ec932}

/*a*/
a{ text-decoration:none}
a,a:active {outline: none;text-decoration:none }
a{ -webkit-tap-highlight-color: rgba(0,0,0,0);}
a:hover{ text-decoration:none;}
a:focus, a:hover{ text-decoration:none;}
/*h*/
h1,h2,h3,h4,h5,h6{ font-weight:normal}

/*字体大小*/
.ft10{ font-size:10px;}
.ft12{ font-size:12px;}
.ft14{ font-size:14px;}
.ft16{ font-size:16px;}
.ft18{ font-size:18px;}
.ft20{ font-size:20px;}
.ft22{ font-size:22px;}
.ft24{ font-size:24px;}
.ft26{ font-size:26px;}
.ft28{ font-size:28px;}
.ft30{ font-size:30px;}
.ft32{ font-size:32px;}
.ft34{ font-size:34px;}
.ft36{ font-size:36px;}
.ft38{ font-size:38px;}

/*padding*/
.pd5{ padding-top:5px; padding-bottom:5px}
.pd10{ padding-top:10px; padding-bottom:10px}
.pd15{ padding-top:15px; padding-bottom:15px}
.pd20{ padding-top:20px; padding-bottom:20px}
.pd25{ padding-top:25px; padding-bottom:25px}
.pd30{ padding-top:30px; padding-bottom:30px}
.pd40{ padding-top:40px; padding-bottom:40px}

/*margin*/
.mt5{ margin-top:5px}
.mt10{ margin-top:10px}
.mt15{ margin-top:15px}
.mt20{ margin-top:20px}
.mt25{ margin-top:25px}
.mt30{ margin-top:30px}
.mt40{ margin-top:40px}
.mt50{ margin-top:50px}

/*暂无记录*/
.new_wjl{ text-align:center; line-height:100px;}

/*按钮*/
.table .btn{ margin-left:5px}
.btn-lg{ padding: 6px 16px; font-size: 16px;}

/*tab*/
.tabbable-line > .nav-tabs{ border-bottom:1px solid #f0f0f0; width:100%}
.tabbable-line > .nav-tabs > li{ padding:0 5px}
.tabbable-line > .nav-tabs > li.active{border-bottom:4px solid #71b7e6}
.tabbable-line > .nav-tabs > li.active a{ color:#3598dc; font-weight:700}
.tabbable-line > .nav-tabs > li.open, .tabbable-line > .nav-tabs > li:hover {background: none;border-bottom: 4px solid #c6e2f5; }
.tabbable-line > .nav-tabs > li.active{border-bottom: 4px solid #71b7e6;}


