package com.caishen91.jupiter.model;

public class BlogManagerPermit extends BaseEntity {

	private static final long serialVersionUID = 1L;
	/**
	 * 	管理员ID
	 */
	private int bmId;
	/**
	 * 	商户ID
	 */
	private int blogId;
	/**
	 * 	权限ID
	 */
	private int bpId;
	
	
	public int getBmId() {
		return bmId;
	}
	public void setBmId(int bmId) {
		this.bmId = bmId;
	}
	public int getBlogId() {
		return blogId;
	}
	public void setBlogId(int blogId) {
		this.blogId = blogId;
	}
	public int getBpId() {
		return bpId;
	}
	public void setBpId(int bpId) {
		this.bpId = bpId;
	}
	
	
}
