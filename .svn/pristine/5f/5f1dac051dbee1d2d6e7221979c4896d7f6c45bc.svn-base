package com.caishen91.jupiter.util;

public class Action {
	private String op;
	
	private String url;
	
	private String target= "_self";
	
	private String opType = "script";
	
	private String data_url = ""; //记录异步请求的url
	
	private String req_type = ""; //异步请求类别 link 直接访问 div 弹层
	
	private String div_type; //弹层类别id
	
	private String confirm_desc;//cconfirm确认描述
	
	private String tag;//菜单样式跳转
	
	private String hf;//操作完后跳转页面链接
	 
	public String getHf() {
		return hf;
	}

	public void setHf(String hf) {
		this.hf = hf;
	}

	public String getTag() {
		return tag;
	}

	public void setTag(String tag) {
		this.tag = tag;
	}

	public String getConfirm_desc() {
		return confirm_desc;
	}

	public void setConfirm_desc(String confirm_desc) {
		this.confirm_desc = confirm_desc;
	}

	public String getDiv_type() {
		return div_type;
	}

	public void setDiv_type(String div_type) {
		this.div_type = div_type;
	}

	public String getReq_type() {
		return req_type;
	}

	public void setReq_type(String req_type) {
		this.req_type = req_type;
	}

	public String getData_url() {
		return data_url;
	}

	public void setData_url(String data_url) {
		this.data_url = data_url;
	}

	public String getOp() {
		return op;
	}

	public void setOp(String op) {
		this.op = op;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getTarget() {
		return target;
	}

	public void setTarget(String target) {
		this.target = target;
	}

	public String getOpType() {
		return opType;
	}

	public void setOpType(String opType) {
		this.opType = opType;
	}
}
