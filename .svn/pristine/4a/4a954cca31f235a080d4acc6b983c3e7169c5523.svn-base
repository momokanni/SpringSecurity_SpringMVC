@charset "utf-8";
/* CSS Document */
*{ margin:0px; padding:0px; list-style:none;}
body{ font-size:14px; color:#666; font-family:"PingFang SC","Microsoft YaHei",HelveticaNeue-Thin,"Helvetica Neue",Helvetica,Arial; overflow-x:hidden;}
img{width:100%; width:auto\9;height:auto; vertical-align:middle; border:0; -ms-interpolation-mode:bicubic;}
a,a:active {color:#7c7f85; text-decoration:none}
a,a:hover,a:active {outline: none;text-decoration:none }
a,a:hover,a:active{ -webkit-tap-highlight-color: rgba(0,0,0,0);}
input,textarea{outline:none;}

.widthall{ width:100%;}
.widbig{ width:auto; padding:0 4%}

/*bgcolor*/
.bgff{background:#ffffff;}/*白色*/
.bgf7{background:#f7f7f7;}/*浅灰*/
.bggre{ background:#31c27c}/*绿色*/
.appdiv{position:fixed; height:100%; padding:15px 0px 0px 0px; background-color:#000; filter:alpha(opacity=60); -moz-opacity: 0.6; opacity: 0.6; left: 0; top: 0px;bottom:0px; width: 100%; z-index: 101;}/*黑色半透明层*/
.tslayer{ background:#000; filter:alpha(opacity=60); -moz-opacity: 0.6; opacity: 0.6; padding:15px 0px; position:fixed; top:35%; color:#fff; font-size:16px; text-align:center; z-index:2;}

/*button*/
.btn_red,.btn_org,.btn_gre,.btn_e7,.btn_gre_l{ height:44px; display:block; line-height:44px; border-radius:22px; text-align:center;}
.btn_red{ background:#fe5050; color:#fff;}
.btn_org{background:#ff683f; color:#fff;}
.btn_gre{ background:#31c27c; color:#fff;}
.btn_e7{ background:#e7e7e7;}
.btn_gre_l{ background:#fff; border:1px #31c27c solid; color:#31c27c;}


/*color*/
.colf{ color:#fff}
.colred{ color:#f75f52}/*red*/
.colblue{ color:#1c6ddb}/*blue*/
.colgre{ color:#31c27c}/*green*/
.colorg{ color:#fe5e2c}/*org*/
.colc{ color:#ccc}
.cola{ color:#aaa}
.col3{color:#333;}
.col4{color:#444;}
.col6{color:#666;}
.col8{color:#888;}
.col9{color:#999;}

/*color_hover*/
.ahov01:hover{ color:#fe5050}/*red*/
.ahov02:hover{ color:#ff683f}/*org*/
.ahov03:hover{ color:#1c6ddb}/*blue*/
.ahov04:hover{ color:#5d646e}/*深*/
.ahov05:hover{ color:#7884a0}/*中*/
.ahov06:hover{ color:#b5bbcb}/*浅*/

/*height width*/
h1,h2,h3,h4,h5,h6{ font-weight:normal}
.h15{height:15px;}
.h24{height:24px;}
.h28{height:28px;}
.h30{height:30px;}
.h40{height:40px;}
.h50{height:50px;}

.hl18{line-height:18px;}
.hl20{line-height:20px;}
.hl24{line-height:24px;}
.hl28{line-height:28px;}
.hl30{line-height:30px;}
.hl40{line-height:40px;}
.hl50{line-height:50px;}

.wd30{width:30%;}
.wd50{width:50%;}
.wd70{width:70%;}
.wd80{width:80%;}
.wd84{width:84%;}
.wd90{width:90%;}
.wd94{width:94%;}
.wd100{ width:100%;}


/*字体大小样式*/
.ft10{ font-size:10px;}
.ft12{ font-size:12px;}
.ft13{ font-size:13px;}
.ft14{ font-size:14px;}
.ft15{ font-size:15px;}
.ft16{ font-size:16px;}
.ft18{ font-size:18px;}
.ft20{ font-size:20px;}
.ft22{ font-size:22px;}
.ft24{ font-size:24px;}
.ft26{ font-size:26px;}
.ft28{ font-size:28px;}
.ft30{ font-size:30px;}
.ft32{ font-size:32px;}
.ft34{ font-size:34px;}
.ft36{ font-size:36px;}
.ft38{ font-size:38px;}
.ft44{ font-size:44px;}
.fit{font-style:italic;}
.far{ font-family:Arial}
.fdec{text-decoration:underline;}
.fw{font-weight:bold;}

/*padding*/
.p5{ padding:5px;}
.p10{ padding:10px;}
.p15{ padding:15px;}
.p20{ padding:20px;}
.p25{ padding:25px;}
.p30{ padding:30px;}
.p40{ padding:40px;}

.pt5{ padding-top:5px;}
.pt10{ padding-top:10px;}
.pt15{ padding-top:15px;}
.pt20{ padding-top:20px;}
.pt25{ padding-top:25px;}
.pt30{ padding-top:30px;}

.pr5{ padding-right:5px;}
.pr10{ padding-right:10px;}
.pr15{ padding-right:15px;}
.pr20{ padding-right:20px;}
.pr30{ padding-right:30px;}

.pb5{padding-bottom:5px;}
.pb10{padding-bottom:10px;}
.pb15{padding-bottom:15px;}
.pb20{padding-bottom:20px;}
.pb30{padding-bottom:30px;}
.pb70{padding-bottom:70px;}


.pl5{ padding-left:5px;}
.pl10{ padding-left:10px;}
.pl15{ padding-left:15px;}
.pl20{ padding-left:20px;}

/*margin*/
.maut{margin:0px auto;}
.maut20{margin:20px auto;}
.mt5{ margin-top:5px}
.mt10{ margin-top:10px}
.mt15{ margin-top:15px}
.mt20{ margin-top:20px}
.mt25{ margin-top:25px}
.mt30{ margin-top:30px}
.mt50{ margin-top:50px}

.mb5{ margin-bottom:5px}
.mb10{ margin-bottom:10px}
.mb15{ margin-bottom:15px}

/*input*/
input{outline: none;  vertical-align:middle; border:none; border-radius:0; background:none}
:-moz-placeholder{color:#ccc; font-size:12px; font-weight:normal}
::-moz-placeholder{color:#ccc; font-size:12px;font-weight:normal}
input:-ms-input-placeholder{color:#ccc; font-size:12px;font-weight:normal}
input::-webkit-input-placeholder{color:#ccc; font-size:12px;font-weight:normal}

/*宽度*/
.colmd-1, .colmd-2, .colmd-3, .colmd-4, .colmd-5, .colmd-6, .colmd-7, .colmd-8, .colmd-9, .colmd-10, .colmd-11, .colmd-12, .colmd-13, .colmd-14, .colmd-15, .colmd-16, .colmd-17, .colmd-18, .colmd-19, .colmd-20{float: left;}
.colmd-20 {width: 100%;}
.colmd-19 {width: 95%;}
.colmd-18 {width: 90%;}
.colmd-17 {width: 85%;}
.colmd-16 {width: 80%;}
.colmd-15 {width: 75%;}
.colmd-14 {width: 70%;}
.colmd-13 {width: 65%;}
.colmd-12 {width: 60%;}
.colmd-11 {width: 55%;}
.colmd-10 {width: 50%;}
.colmd-9 {width: 45%;}
.colmd-8 {width: 40%;}
.colmd-7 {width: 35%;}
.colmd-6 {width: 30%;}
.colmd-5 {width: 25%;}
.colmd-4 {width: 20%;}
.colmd-3 {width: 15%;}
.colmd-2 {width: 10%;}
.colmd-1 {width: 5%;}

/*table tit*/
.tab_tit{ height:45px; line-height:45px; background:#f0f0f0}
.tab_h30 li{ height:30;}
.tab_h40 li{ height:40;}
.tab_h50 li{ height:50;}
.tab_h60 li{ height:60;}

/*best*/
.fl{ display:block; float:left}
.fr{ display:block; float:right}
.inl_bl{display:inline-block;}
.dis_b{ display:block;}
.pr{ position:relative}
.pa{ position:absolute;}
.over{ height:auto; overflow:hidden}
.tcen{text-align:center;}
.trt{text-align:right;}
.tind{text-indent:2em;}
.borad5{border-radius:5px;}
.borad20{border-radius:20px;}
.bd_t{border-top:1px #ececec solid;}
.bd_t_f2{border-top:1px #f2f2f2 solid;}
.bd_r{border-right:1px #ececec solid;}
.bd_b{border-bottom:1px #ececec solid;}
.bd_b_f2{border-bottom:1px #f2f2f2 solid;}
.bd_l{border-left:1px #ececec solid;}
.clearfix{ display: block; *display:inline-block;}
.clearfix:after{ clear: both; content: "."; display: block; height: 0; overflow: hidden; visibility: hidden;}
.dn{ display:none}
.ellipsis{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.ellipsis2{overflow : hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}


/*遮罩*/
.shade{ position:fixed; bottom:0; left:0; width:100%; height:100%; background:#000; opacity:.6; z-index:100}
.shade02{ opacity:.3}

/*暂无数据*/
.nodata { text-align:center}
.nodata img{ width:24%; margin:0 auto; margin-top:40%}


/*tophead*/
.header{ width:92%; height:44px; border-bottom:1px solid #fff; background:#fff; position:fixed; top:0; left:0; z-index:99;/*position: -webkit-sticky; position: sticky;*/}
.header h3{ text-align:center; height:45px; line-height:45px; font-weight:bold}

.navback{ position:absolute; top:0; left:0%; height:45px}
.navback a,.navfr a{display:block; width:45px; height:45px; float:left;}
.navback img,.navfr img{ display:block; width:24px;margin:0 auto; margin-top:11px}
.navfr{ position:absolute; top:0; right:0;}

/*ios nav*/
.topnav_ios,.topnav_ios .navfr,.topnav_ios .navback{ padding-top:25px}


