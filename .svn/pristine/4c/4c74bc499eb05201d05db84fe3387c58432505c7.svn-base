package com.caishen91.jupiter.model;


public class Blog extends BaseEntity {

	private static final long serialVersionUID = -8543608813550891091L;

	// 公众号名称
	private String name;
	// 公众号描述
	private String description;
	// 账号类型（0.个人号，1.公司号）
	private int type;
	// 企业名称
	private String enterpriseName;
	// 企业类型
	private int enterpriseType;
	// 社会信用代码
	private String scc;
	// 公司邮箱
	private String email;
	// 头像
	private String headImg;

	private String headImgMb;

	// 状态（0.无效，1.有效）
	private int status;
	// 认证状态（0.未认证，1.已认证）
	private int authStatus;
	// 粉丝数
	private int fansCount;
	// 推送接口路径
	private String pushUrl;

	public String getHeadImgMb() {
		return headImgMb;
	}

	public void setHeadImgMb(String headImgMb) {
		this.headImgMb = headImgMb;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getEnterpriseName() {
		return enterpriseName;
	}

	public void setEnterpriseName(String enterpriseName) {
		this.enterpriseName = enterpriseName;
	}

	public int getEnterpriseType() {
		return enterpriseType;
	}

	public void setEnterpriseType(int enterpriseType) {
		this.enterpriseType = enterpriseType;
	}

	public String getScc() {
		return scc;
	}

	public void setScc(String scc) {
		this.scc = scc;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getHeadImg() {
		return headImg;
	}

	public void setHeadImg(String headImg) {
		this.headImg = headImg;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public int getAuthStatus() {
		return authStatus;
	}

	public void setAuthStatus(int authStatus) {
		this.authStatus = authStatus;
	}

	public int getFansCount() {
		return fansCount;
	}

	public void setFansCount(int fansCount) {
		this.fansCount = fansCount;
	}

	public String getPushUrl() {
		return pushUrl;
	}

	public void setPushUrl(String pushUrl) {
		this.pushUrl = pushUrl;
	}
}
