.file_progress {
  background: url("/images/source/user/userbtn_n.png") no-repeat 0px -454px;
  width: 120px;
  height: 36px;
  line-height: 36px;
  text-align: center;
}

.pic_list{ width:578px; clear:both;text-align:center; padding-top:8px; margin-bottom:8px;}
.four{width:578px;}
.five{width:664px;}
.pic_list dl{ width:140px; float:left; display:inline; margin-right:22px; margin-bottom:22px; }
.five dl{ margin-right:50px;}
.pic_list dl dt img{ width:118px; height:80px; border:1px solid #6F6D6E;}
.pic_list .upload{height:82px; background-color:#F1F1F1; border-bottom:19px solid #fff;}
.five .upload{border-bottom:17px solid #fff;_border-bottom:15px solid #fff;}
.pic_list .upload dt{ color:#535353; margin-top:2px;}
.pic_list .upload .home{ margin-top:16px;}
.pic_list .upload dd{ margin:4px 0; text-align:center;}
.pic_list .upload dd em{ background-color:#0069CA; width:103px; height:7px; display:block; margin:0 auto;}