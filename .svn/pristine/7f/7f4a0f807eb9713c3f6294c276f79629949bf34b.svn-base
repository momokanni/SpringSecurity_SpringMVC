    background-position: 70px -725px;
}
.ticket-type-select2 {
    width: auto;
}
.ticket-type-select2 .csStitle, .ticket-type-select2 .csContent .csSlist a {
    text-align: left;
    text-indent: 5px;
    width: 70px;
}
.ticket-type-select2 input {
    border: 0 none;
    width: 50px;
}
.csSelect {
    float: left;
    width: 60px;
}
.csStitle {
    background-color: #fff;
    background-position: 34px -725px;
    border: 1px solid #d5d5d5;
    cursor: pointer;
    height: 18px;
    line-height: 18px;
    padding: 3px 18px 3px 3px;
    text-align: center;
    width: 32px;
}
.csContent {
    position: relative;
    width: 60px;
    z-index: 9999;
}
.csContent .csSlist {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #fff;
    border-color: -moz-use-text-color #d5d5d5 #d5d5d5;
    border-image: none;
    border-right: 1px solid #d5d5d5;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    overflow-x: hidden;
    overflow-y: auto;
    position: absolute;
    width: auto;
}
.csContent .csSlist li {
    line-height: 18px !important;
    margin: 0 !important;
}
.csContent .csSlist a {
    color: #555;
    display: block;
    outline: medium none;
    padding: 3px 18px 3px 3px;
    text-align: center;
    width: 32px;
}
.csContent .csSlist a:hover {
    background: none repeat scroll 0 0 #0069ca;
    color: #fff;
}
.csStitle {
    background: url(" //www.xiaoyoucai.com/source/images/manager/skin.png") no-repeat scroll 0 0 #fff;
}


