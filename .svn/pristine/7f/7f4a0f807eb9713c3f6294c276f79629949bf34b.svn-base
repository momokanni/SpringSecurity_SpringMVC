/*left*/
.left{ width:158px; float:left; background:#fff; border:1px #dddddd solid; border-radius:5px;}
.left h3{ font:14px microsoft yahei,Arial; height:38px; border-bottom:1px #f0f0f0 solid; padding:5px 10px;}
.left h3.nobd{border-bottom:0px;}
.left h3 a{ display:block; height:38px; padding-left:23px; line-height:38px; color:#555; border-radius:5px;}
.left h3 a:hover{color:#55a400;}
.left h3 a.on{font-weight:bold; color:#FFF; background:#8cc026;}
.left h3 a.secmn{background:url(/front/images/manager/tts/lfnav.jpg) no-repeat 0px -124px;}
.left h3.tree-node,.left h3 a.secmn:hover{margin-bottom:0px; background-position:0px -158px;}
.left h3.tree-node{position:relative;}
.left h3.tree-node i{width:15px; height:15px; position:absolute; right:14px; top:17px; background:url( //www.xiaoyoucai.com/source/images/manager/add_i.png) 0px 0px no-repeat;}
.left h3.tree-node i.on{background-position:-15px 0px;}
.left h3.tree-node a.on,.left h3.tree-node a.on:hover{background:#fff; color:#55a400;}
.left ul{background:#fefefe; border-bottom:1px #ddd solid; padding:10px 0px;}
.left ul li{ display:block; height:38px; font-size:14px; position:relative; padding:0px 10px;}
.left ul li a{color:#666; display:block; height:38px; line-height:38px; border-radius:5px; padding-left:23px;}
.left ul li a:hover{color:#55a400;}
.left ul li a.on{font-weight:bold; color:#FFF; height:38px; border-bottom:0px; background-position:0px 0px; background:#8cc026;}





